{
	"name": "groupie-tracker",
	"version": "1.0.4",
	"description": "An artists data collection",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "eslint --fix .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"vue": "^3.2.0"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^4.0.0",
		"eslint": "^8.0.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-vue": "^9.0.0",
		"jsdom": "^21.0.0",
		"prettier": "^3.0.0",
		"vite": "^4.0.0",
		"vitest": "^0.30.1"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es2021": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:vue/vue3-recommended",
			"prettier"
		],
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"rules": {
			"no-console": "warn",
			"no-unused-vars": "warn"
		}
	},
	"engines": {
		"node": ">=14.0.0"
	}
}
